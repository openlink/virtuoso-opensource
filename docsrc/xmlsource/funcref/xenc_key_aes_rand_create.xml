    <errorcode></errorcode>
  </refsect1>
  -->
  <refsect1 id="examples_xenc_key_AES_rand_create">
    <title>Example</title>
    <example id="ex_xenc_key_AES_rand_create"><title></title>
      <screen><![CDATA[
xenc_key_AES_rand_create ('ses_key', 192);]]>
</screen>
    </example>
  </refsect1>
