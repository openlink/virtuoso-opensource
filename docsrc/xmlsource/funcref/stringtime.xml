<refentry id="fn_stringtime">
  <refmeta>
    <refentrytitle>stringtime</refentrytitle>
    <refmiscinfo>time</refmiscinfo>
    <refmiscinfo>type</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>stringtime</refname>
    <refpurpose>converts string to a time</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_stringtime">
      <funcprototype id="fproto_stringtime">
        <funcdef>time <function>stringtime</function></funcdef>
        <paramdef>in <parameter>str</parameter> varchar</paramdef>
      </funcprototype>
    </funcsynopsis>

	</refsynopsisdiv>
  <refsect1 id="desc"><title>Description</title>
    <para>Converts the argument to a time. Same as</para>
    <programlisting>cast (x as time)</programlisting>
  </refsect1>
</refentry>


