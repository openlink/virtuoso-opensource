<refentry id="fn_xenc_key_remove">
  <refmeta>
    <refentrytitle>xenc_key_remove</refentrytitle>
    <refmiscinfo>hash</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>xenc_key_remove</refname>
    <refpurpose>This will delete a key from current user's space.</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_xenc_key_remove">
      <funcprototype id="fproto_xenc_key_remove">
        <funcdef><function>xenc_key_remove</function></funcdef>
	<paramdef>in <parameter>key_name</parameter> varchar</paramdef>
      </funcprototype>
    </funcsynopsis>
  </refsynopsisdiv>
  <refsect1 id="desc_xenc_key_remove">
    <title>Description</title>
    <para>This will delete a key from current user's space.</para>
  </refsect1>
  <refsect1 id="params_xenc_key_remove">
    <title>Parameters</title>
    <refsect2><title>key_name</title>
      <para>The name of the key.</para>
    </refsect2>
  </refsect1>
  <refsect1 id="examples_xenc_key_remove">
    <title>Example</title>
    <example id="ex_xenc_key_remove"><title>Simple Use</title>
      <screen><![CDATA[
xenc_key_remove ('file:dsa.der');]]>
</screen>
    </example>
  </refsect1>
  <refsect1 id="seealso_xenc_key_remove">
    <title>See Also</title>
    <para><link linkend="fn_xenc_key_exists"><function>xenc_key_exists()</function></link></para>
  </refsect1>
</refentry>
