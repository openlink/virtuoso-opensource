<refentry id="fn_concatenate">
  <refmeta>
    <refentrytitle>concatenate</refentrytitle>
    <refmiscinfo>array</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>concatenate</refname>
    <refpurpose>concatenate strings</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_concatenate">
      <funcprototype id="fproto_concatenate">
        <funcdef>string <function>concatenate</function></funcdef>
        <paramdef>in <parameter>arg_1</parameter> any</paramdef>
        <paramdef><parameter>...</parameter></paramdef>
      </funcprototype>
    </funcsynopsis>
  </refsynopsisdiv>
  <refsect1 id="desc_concatenate">
    <title>Description</title>
      <para><function>Concatenate</function> is an alias of
      <link linkend="fn_concat"><function>concat</function></link>.</para>
  </refsect1>
  <refsect1 id="seealso_concatenate"><title>See Also</title>
      <para><link linkend="fn_concat"><function>concat</function></link></para>
  </refsect1>
</refentry>



