<refentry id="fn_ST_Z">
  <refmeta>
    <refentrytitle>ST_Z</refentrytitle>
    <refmiscinfo>geo</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>ST_Z</refname>
    <refpurpose>Retrieves the z coordinate of a geometry.</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_ST_Z">
      <funcprototype id="fproto_ST_Z">
        <funcdef><function>ST_Z</function></funcdef>
	        <paramdef>in <parameter>g</parameter> any</paramdef>
      </funcprototype>
    </funcsynopsis>
  </refsynopsisdiv>
  <refsect1 id="desc_ST_Z">
    <title>Description</title>
    <para>Retrieves the x coordinate of a geometry.</para>
  </refsect1>
  <refsect1 id="params_ST_Z">
    <title>Parameters</title>
    <refsect2><title>g</title>
      <para>Geometry</para>
    </refsect2>
  </refsect1>
  <refsect1 id="ret_ST_Z"><title>Return Types</title>
    <para>Returns double precision.</para>
  </refsect1>
  <refsect1 id="examples_ST_Z">
    <title>Examples</title>
    <example id="ex_ST_Z"><title>Simple Use</title>
      <screen><![CDATA[
SELECT st_z( st_point (10, 20, 30, 40));
DOUBLE PRECISION
 30
]]>
</screen>
    </example>
  </refsect1>
  <refsect1 id="seealso_ST_Z">
    <title>See Also</title>
    <para><link linkend="fn_st_x"><function>ST_X()</function></link></para>
