<refentry id="fn_exec_close">
  <refmeta>
    <refentrytitle>close</refentrytitle>
    <refmiscinfo>sql</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>exec_close</refname>
    <refpurpose>Closes cursor created by exec()</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_exec_close">
      <funcprototype id="fproto_exec_close">
        <funcdef><function>exec_close</function></funcdef>
        <paramdef>in <parameter>cursor_handle</parameter> long</paramdef>
			
      </funcprototype>
    </funcsynopsis>
  </refsynopsisdiv>
  <refsect1 id="desc_exec_rclose"><title>Description</title>
    <para>Closes the cursor opened by the exec() function.</para>
  </refsect1>
  <refsect1 id="params_exec_rclose">
    <title>Parameters</title>
    <refsect2>
      <title>cursor_handle</title>
      <para>The cursor handle used.</para>
    </refsect2>
  </refsect1>
  <refsect1 id="seealso_exec_close"><title>See Also</title>
    <para><link linkend="fn_exec_next"><function>exec_next()</function></link></para>
    <para><link linkend="fn_exec"><function>exec()</function></link></para>
    </refsect1>
</refentry>
