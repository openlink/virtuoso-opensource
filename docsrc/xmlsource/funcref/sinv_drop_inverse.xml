<refentry id="fn_sinv_drop_inverse">
  <refmeta>
    <refentrytitle>sinv_drop_inverse</refentrytitle>
    <refmiscinfo>sql</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>sinv_drop_inverse</refname>
    <refpurpose>Reverses the effect of <link linkend="fn_sinv_create_inverse"><function>sinv_create_inverse</function></link> procedure.</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_sinv_drop_inverse">
      <funcprototype id="fproto_sinv_drop_inverse">
        <funcdef><function>sinv_drop_inverse</function></funcdef>
	<paramdef>in <parameter>_SINVM_NAME_IN</parameter> varchar</paramdef>
      </funcprototype>
    </funcsynopsis>
  </refsynopsisdiv>$
  <refsect1 id="desc_sinv_drop_inverse"><title>Description</title>
    <para>Reverses the effect of <link linkend="fn_sinv_create_inverse"><function>sinv_create_inverse</function></link> procedure.</para>
  </refsect1>
  <refsect1 id="params_sinv_drop_inverse">
    <title>Parameters</title>
    <refsect2><title>_SINVM_NAME_IN</title>
      <para>the name of the forward function</para></refsect2>
  </refsect1>
  <refsect1 id="seealso_sinv_drop_inverse"><title>See Also</title>
    <para>
      <link linkend="fn_sinv_create_key_mapping"><function>sinv_create_key_mapping</function></link>,
      <link linkend="fn_sinv_create_inverse"><function>sinv_create_inverse</function></link>
    </para>
  </refsect1>
</refentry>
