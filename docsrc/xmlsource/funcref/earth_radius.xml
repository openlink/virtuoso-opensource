<refentry id="fn_earth_radius">
  <refmeta>
    <refentrytitle>earth_radius</refentrytitle>
    <refmiscinfo>geo</refmiscinfo>
  </refmeta>
  <refnamediv>
    <refname>earth_radius</refname>
    <refpurpose>returns geom.mean of radius of Earth</refpurpose>
  </refnamediv>
  <refsynopsisdiv>
    <funcsynopsis id="fsyn_earth_radius">
      <funcprototype id="fproto_earth_radius">
        <funcdef><function>earth_radius</function></funcdef>
        <paramdef></paramdef>
      </funcprototype>
    </funcsynopsis>
  </refsynopsisdiv>
  <refsect1 id="desc_earth_radius">
    <title>Description</title>
    <para>returns geom.mean of radius of Earth in kilometers, 6367.43568</para>
  </refsect1>
  <refsect1 id="ret_earth_radius"><title>Return Types</title>
    <para>Returns any.</para>
  </refsect1>
  <refsect1 id="examples_earth_radius">
    <title>Examples</title>
    <example id="ex_earth_radius"><title>Simple Use</title>
<programlisting><![CDATA[
SELECT earth_radius ()
earth_radius
ANY
 6367.43568
]]></programlisting>
    </example>
