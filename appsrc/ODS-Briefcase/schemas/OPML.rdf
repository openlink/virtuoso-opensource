<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl ="http://www.w3.org/2002/07/owl#"
         xmlns:virtrdf="http://www.openlinksw.com/schemas/virtrdf#"
         xmlns:rss="http://www.python.org/topics/xml/xbel/"
	 >
  <owl:Ontology rdf:about="http://www.openlinksw.com/schemas/OPML#">
        <rdfs:label>Aggregated Content Channels (OPML, OCS)</rdfs:label>
        <rdfs:comment>Syndicated Content Aggregation using OPML or OCS aggregation formats</rdfs:comment>
        <virtrdf:catName>Aggregated Content Channels</virtrdf:catName>
    <virtrdf:version>1.00</virtrdf:version>
  </owl:Ontology>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OPML#title">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Title</virtrdf:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OPML#dateCreated">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Creation Date</virtrdf:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OPML#dateModified">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Modification Date</virtrdf:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OPML#ownerName">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Owner Name</virtrdf:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OPML#ownerEmail">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Owner e-mail</virtrdf:label>
  </rdf:Property>
</rdf:RDF>
