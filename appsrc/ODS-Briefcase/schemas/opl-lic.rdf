<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl ="http://www.w3.org/2002/07/owl#"
         xmlns:virtrdf="http://www.openlinksw.com/schemas/virtrdf#"
         >
  <owl:Ontology rdf:about="http://www.openlinksw.com/schemas/OplLic#">
    <rdfs:label>OpenLink License</rdfs:label>
    <rdfs:comment>License files</rdfs:comment>
    <virtrdf:catName>License File</virtrdf:catName>
    <virtrdf:version>1.00</virtrdf:version>
  </owl:Ontology>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OplLic#RegisteredTo">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Registered To</virtrdf:label>
    <virtrdf:catName>Username</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OplLic#ConnectionNumber">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Number of connections</virtrdf:label>
    <virtrdf:catName>Connections</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/OplLic#SerialNumber">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Serial number</virtrdf:label>
    <virtrdf:catName>Serial</virtrdf:catName>
  </rdf:Property>
</rdf:RDF>
