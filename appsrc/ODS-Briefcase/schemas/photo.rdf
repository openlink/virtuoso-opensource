<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl ="http://www.w3.org/2002/07/owl#"
         xmlns:virtrdf="http://www.openlinksw.com/schemas/virtrdf#"
         xml:base="http://www.openlinksw.com/schemas/Photo#">
  <owl:Ontology rdf:about="http://www.openlinksw.com/schemas/Photo#">
    <rdfs:label>Photos</rdfs:label>
	<rdfs:comment>Camera generated still images that includes EXIF metadata</rdfs:comment>
    <virtrdf:catName>Photos</virtrdf:catName>
    <virtrdf:version>1.00</virtrdf:version>
  </owl:Ontology>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Photo#type">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Type</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Type</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Photo#size">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Size</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Size</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Photo#dimensions">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Dimensions</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Dimensions</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Photo#resolutions">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Resolutions</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Resolutions</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Photo#depth">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Depth</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Depth</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Photo#comments">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Comments</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Comments</virtrdf:catName>
  </rdf:Property>
</rdf:RDF>
