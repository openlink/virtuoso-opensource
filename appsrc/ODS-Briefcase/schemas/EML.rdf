<rdf:RDF xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl ="http://www.w3.org/2002/07/owl#"
         xmlns:virtrdf="http://www.openlinksw.com/schemas/virtrdf#"
         xml:base="http://www.openlinksw.com/schemas/Email#">
  <owl:Ontology rdf:about="http://www.openlinksw.com/schemas/Email#">
    <rdfs:label>Emails</rdfs:label>
    <rdfs:comment>The email file format general attributes.</rdfs:comment>
    <virtrdf:catName>Emails</virtrdf:catName>
    <virtrdf:version>1.00</virtrdf:version>
  </owl:Ontology>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Email#from">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>From</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>From</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Email#subject">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Subject</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Subject</virtrdf:catName>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.openlinksw.com/schemas/Email#date">
    <rdfs:Range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <virtrdf:cardinality>single</virtrdf:cardinality>
    <virtrdf:label>Date</virtrdf:label>
    <virtrdf:defaultValue>No name</virtrdf:defaultValue>
    <virtrdf:catName>Date</virtrdf:catName>
  </rdf:Property>
</rdf:RDF>
