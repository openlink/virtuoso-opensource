                          }
                          else
                          {
                            declare N, M, V any;

                            -- dbg_obj_print ('', self.dav_detType);
                            V := vector (
                                          0, '',           'Normal',
                                          1, 'ResFilter',  'Smart Folder',
                                          1, 'CatFilter',  'Category Folder',
                                          1, 'PropFilter', 'Property Filter Folder',
                                          1, 'HostFs',     'Host FS Folders',
