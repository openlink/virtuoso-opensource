        <div class="form-confirm">
          <?vsp
            declare old_vector any;

            old_vector := ODRIVE.WA.DAV_INIT (self.dav_vector[0]);
            -- self.dav_vector := vector (dav_fullPath, dav_file, dav_mime, dav_perms, dav_owner, dav_group);
