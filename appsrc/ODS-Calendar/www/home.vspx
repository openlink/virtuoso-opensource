                        </td>
                        </vm:if>
                        <vm:if test="self.columnEnabled ('task_E_COMPLETED', 0)">
                        <td nowrap="nowrap">
                                    <?vsp self.showEventEnd (e_event, rowset[11], 'completed'); ?>
                        </td>
                        </vm:if>
                        <vm:if test="self.columnEnabled ('task_E_STATUS', 1)">
