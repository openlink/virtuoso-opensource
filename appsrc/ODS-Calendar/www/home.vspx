            </vm:if>
            <vm:if test="self.cScope = 'search'">
                        <td style="padding: 0 2px 0 2px;">
                          <?vsp
                                    if (e_kind = 0)
                              http ('Event');
                                    else if (e_kind = 1)
                              http ('Task');
                                    else if (e_kind = 2)
                              http ('Note');
                          ?>
                        </td>
