                  }
                  else if (self.cAction = 'subscribeCreate')
                  {
                    T := 'Subscriptions/Create';
                  }
                  else if (self.cAction = 'subscribeUpdate')
                  {
                    T := 'Subscriptions/Update';
                  }
                  else if (self.cAction = 'syncmlCreate')
                  {
                    T := 'SyncML/Create';
                  }
                  else if (self.cAction = 'syncmlUpdate')
