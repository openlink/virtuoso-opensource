              <vm:if test="self.account_rights = 'W'">
                <vm:event />
              </vm:if>
              <vm:calendar />
              <vm:bookmarklet />
              <vm:calendars />
              <vm:exchange />
              <vm:formats />
            </vm:if>
            <vm:if test="self.cAction = 'settings'">
              <div class="lc lc_close">
