                    <![CDATA[
                      self.i_maps := null;
                      self.vc_data_bind(e);
                    ]]>
                  </v:on-post>
                </v:button>
              </div>
	            <div class="new-form-footer">
                  <v:button action="simple" name="i4_back" value="Back" xhtml_class="form-button">
