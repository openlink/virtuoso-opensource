                    {
                      declare exit handler for not found goto _end2;

                      if (self.abSubAction in  ('publishUpdate', 'subscribeUpdate', 'syncmlUpdate'))
                      {
                        select EX_TYPE,
                               EX_NAME,
                               EX_UPDATE_TYPE,
