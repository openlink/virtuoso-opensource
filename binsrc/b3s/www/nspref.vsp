  <head>
    <link rel="stylesheet" type="text/css" href="style.css">
    <title><?=label?></title>
  </head>
  <body>
    <div id="header">
      <div id="hd_l">
        <h1 id="title"><?vsp http (sprintf ('%s', label)); ?></h1>
      </div> <!-- hd_l -->
    </div> <!-- header -->
    <div id="content">
      <table class="tableresult" border="1">
        <?vsp
	http (sprintf ('<tr><th>Prefix</th><th>URI</th></tr>'));
