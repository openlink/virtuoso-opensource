	</tr>
        <tr>
	  <td CLASS="statlisthead">Early timeout</td>
	  <td CLASS="statdata"><?= sys_stat('tws_early_timeout') ?></td>
	</tr>
    </table>
<?vsp adm_page_footer(); ?>

