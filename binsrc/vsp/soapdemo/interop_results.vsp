  declare _pass, _no_pass, _all integer;
  declare _is_empty integer;

  if (exists (select 1 from RESULTS))
    _is_empty := 0;
  else
    _is_empty := 1;

?>

<!DOCTYPE HTML PUBLIC "-//IETF//DTD HTML//EN">

<HTML>
  <head>
    <link rel="stylesheet" type="text/css" href="<?=adm_what_css()?>">
    <title>User Accounts</title>
    <meta name="AUTHOR" content="OpenLink Software">
    <meta name="DESCRIPTION" content="Interop Results page">
  </head>
  <BODY>
  <H1>OpenLink SOAP Client 1.1</H1>
  <P>&nbsp;
  <P><H3>View results by servers</H3>
  <TABLE>
<?vsp
   if (_is_empty) http ('<TR><TD>The result table is empty.</TD></TR>');
   for (select R_SERVER_NAME as SERVER_NAME from RESULTS group by R_SERVER_NAME order by R_SERVER_NAME) do
     {
        select count (*) into _all from RESULTS where R_SERVER_NAME = SERVER_NAME;
        select count (*) into _pass from RESULTS where R_SERVER_NAME = SERVER_NAME and R_SERVICE_RESULT = 'Passed';

        _no_pass := _all - _pass;
?>
        <TR>
	  <TD>
	    <A href="interop_results2.vsp?SRVNAME=<?=encode_base64(SERVER_NAME)?>"><?=SERVER_NAME?></A>
          </TD>
	  <TD> <?vsp if (_no_pass) http ('<font style="color:red">');?>
	    <?=_pass?>
          </TD>
	  <TD>
	    / <?=_all?>
          </TD>
	</TR>
<?vsp
     }
?>
  </TABLE>
  </BODY>
</HTML>

