    <link rel="stylesheet" href="<?=skinpfx?>/ss/sponger_frontpage.css" type="text/css"/>
    <?vsp if (fp_favicon <> 0) { ?>
    <link rel="shortcut icon" href="<?=skinpfx?>/i/<?=fp_favicon?>" />
    <?vsp } ?>
