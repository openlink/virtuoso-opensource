<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:sql="urn:schemas-openlink-com:xml-sql">
    <sql:xquery sql:context="virt://XQ.XQ.TEST_FILES.NAME.TEXT:xqdemo/bib.xml"><![CDATA[
	for  $b in /bib/book[@year > 1990]
		where $b/publisher = 'Addison-Wesley'
			return $b/title
	]]></sql:xquery>
</root>
