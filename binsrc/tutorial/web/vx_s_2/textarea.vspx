<html>
  <head>
    <link href="../demo.css" type="text/css" rel="stylesheet"/>
  </head>
  <body>
    <p>
      <v:page name="textarea_demo" xmlns:v="http://www.openlinksw.com/vspx/">
        <table class="tableentry">
          <v:form name="form_name" type="simple" method="POST" action="">
            <tr>
              <td>
                <v:textarea name="ta1" default_value="enter your text here" value="--coalesce ({?'ta1'}, control.ufl_value)" xhtml_cols="50" xhtml_rows="5">
                  <v:validator test="length" min="0" max="50" message="max 50 chars."/>
                </v:textarea>
              </td>
            </tr>
            <tr>
              <td>
                <v:button name="bt1" action="simple" value="OK"/>
              </td>
            </tr>
          </v:form>
        </table>
      </v:page>
    </p>
  </body>
</html>
