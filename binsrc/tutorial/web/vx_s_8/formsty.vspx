<v:page name="form_styling_demo" decor="formdecor.vspx" style="formsty.xsl" xmlns:v="http://www.openlinksw.com/vspx/" xmlns:xhtml="http://www.w3.org/1999/xhtml">
  <v:style name="pagetitle">
    <p>Header</p>
  </v:style>
  <H3>VSPX macro expansion</H3>
  <p>
    <font style="color:red">
      <v:error-summary/>
    </font>
  </p>
  <v:style name="federalblue">
    <v:form name="sf" type="simple" action="formsty.vspx" method="POST">
      <v:text name="txt2" type="hidden"/>
      <P>
        <v:label name="label1" value="--concat ('name of current control : ', control.vc_name)"/>
      </P>
      <table>
        <tr>
          <td>
            <v:text name="txt" xhtml_size="10"/>
            <v:text name="txt1" xhtml_size="10"/>
          </td>
        </tr>
        <tr>
          <td>
            <v:style name="zigzag">
              <v:button name="submit2" action="simple" value="OK"/>
            </v:style>
          </td>
        </tr>
      </table>
    </v:form>
  </v:style>
</v:page>
