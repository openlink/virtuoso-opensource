<?xml version="1.0" encoding="iso-8859-1"?>
<?xml-stylesheet href="attr-list.css" type="text/css"?>
<!-- compliance patch for microsoft browsers -->
<!--[if lt IE 7]><html:script src="../ie7-standard.js" type="text/javascript"></html:script><![endif]-->
<!-- dean.edwards/2004 -->
<!-- keeping code tidy! -->
<my:root xmlns:my="http://dean.edwards.name/space">
	<my:h1>IE7</my:h1>
	<my:h2>[attr~="value"]</my:h2>
	<my:test>
		<my:shopping list="tea">Tea</my:shopping>
		<my:shopping list="coffee">Coffee</my:shopping>
		<my:shopping list="bananas">Bananas</my:shopping>
		<my:shopping list="tea coffee">Tea and coffee</my:shopping>
		<my:shopping list="tea bananas">Tea and bananas</my:shopping>
		<my:shopping list="coffee bananas">Coffee and bananas</my:shopping>
		<my:shopping list="tea coffee bananas">Tea, coffee and bananas</my:shopping>
	</my:test>
	<my:p>Bananas are yellow :-)</my:p>
</my:root>
