				       len := length( self.foreign_key_columns);
				        i := 0;
				       while (i < len) {
			     	    if  ( current_column = aref(self.foreign_key_columns,i)  and aref(self.foreign_key_columns,i+2) is not null)
			     		    self.reference_table_value :=  aref(self.foreign_key_columns,i+2);
				        i:= i +3;
				       }
			      }
						self.vc_data_bind(e);
			      return;
