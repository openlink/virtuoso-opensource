     <vm:row>
      <vm:bgroup colspan="4" align="center">
       <v:button action="submit" name="service_cancel" value="Cancel" xtml_width="32"/>
       <v:button action="submit" name="service_save" value="Save" xtml_width="32"/>
      </vm:bgroup>
     </vm:row>
    </vm:form>
