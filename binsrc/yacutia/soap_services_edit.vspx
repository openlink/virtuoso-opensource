<v:page name="soap_services_edit_page" decor="yacutia_decor.vspx" style="yacutia_style.xsl" doctype="-//W3C//DTD XHTML 1.0 Transitional//EN" fast-render="1" xmlns:v="http://www.openlinksw.com/vspx/" xmlns:vm="http://www.openlinksw.com/vspx/macro">
  <script type="text/javascript">
    <![CDATA[
      function doBrowse(target,fld_name, startPath, isDir, isDav)
      {
        window._result = document.frm_edit_path[fld_name];
        window.open(target + '&ppath=' +
                    document.frm_edit_path[fld_name].value +
                    '&caption=Choose+' + (isDir ? 'path' : 'resource') +
                    '&dir-separator=/&quote-char=&filter-char=%25&content-proc=' +
                    (isDav ? 'db.dba.dav_browse_proc' : 'db.dba.fs_browse_proc') +
                    '&content-meta-proc=' +
                    (isDav ? 'db.dba.dav_browse_proc_meta' : 'db.dba.fs_browse_proc_meta') +
                    '&crfolder-proc=' +
                    (isDav ? 'db.dba.dav_crfolder_proc' : 'db.dba.fs_crfolder_proc') +
                    '&multi-sel=0&dir-sel=' + (isDir ? '1':'0') +
                    '&start-path=' + (isDav ? '/DAV' : startPath )+ '&retname=_result',
                    'window',
                    'scrollbars=yes, resizable=yes, menubar=no, height=500, width=500');
      }

      function defineOpts (val)
      {
        if ('ssl' == val.options[val.selectedIndex].value)
        {
          if (document.frm_edit_path.t_auth_opt.value == '')
          {
            document.frm_edit_path.t_auth_opt.value =
              'https_cert=[file_with_certificate].pem;\nhttps_key=[file_with_private_key].pem;';
          }
        }
      }
    ]]>
  </script>
  <script language="JavaScript">
    <![CDATA[
    function selectAllCheckboxes (form, btn, filter)
    {
      var i;
      for (i =0; i < form.elements.length; i++)
      {
        var contr = form.elements[i];
        if (contr != null && contr.type == "checkbox")
        {
          if (filter && contr.name != filter)
            ;
          else
          {
            contr.focus();
            if (btn.value == 'Select All')
              contr.checked = true;
            else
              contr.checked = false;
          }
        }
      }
      if (btn.value == 'Select All')
        btn.value = 'Unselect All';
      else
        btn.value = 'Select All';
      btn.focus();
    }
    ]]>
  </script>
