--
-- Process checkboxes, make vectors
--

  self.s_procs := vector ();
  self.s_stables := vector ();
  self.s_tables := vector ();
  self.s_views := vector ();

  if (self.user_tables_flag = 1)
    {
      n := length (self.user_table_list);
