                      <![CDATA[
                        declare i, cnt, len integer;
                        declare tmp_list, tmp_vec any;
                        declare ds_pwdo, ds_pwdn, dsn_name varchar;
                        if (get_keyword ('cancel', self.vc_page.vc_event.ve_params) <> '' or
                            self.vc_is_valid = 0)
                          {
                            error:
                            self.page := self.page_back;
