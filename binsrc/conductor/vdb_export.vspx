<v:page name="db_repl_pub"
  doctype="-//W3C//DTD XHTML 1.0 Transitional//EN"
  fast-render="1" xmlns:v="http://www.openlinksw.com/vspx/"
  xmlns:xhtml="http://www.w3.org/1999/xhtml"
  xmlns:vm="http://www.openlinksw.com/vspx/macro">
    <v:variable name="tblist_enc" persist="0" type="varchar" default="''" param-name="tblist"/>
    <v:variable name="tblist" persist="0" type="any" default="null"/>
    <v:on-init>
	if (self.tblist is null)
	  self.tblist := deserialize (decode_base64 (self.tblist_enc));
      </v:on-init>
    <pre><?vsp http_value (sql_dump_vdb_tables (self.tblist)); ?></pre>
</v:page>
