<html>
  <head>
    <link rel="stylesheet" href="yacutia_style.css" type="text/css"/>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <title>WebDAV Repository</title>
  <script type="text/javascript">
  var featureList = ["combolist", "ajax", "tab"];
  </script>
  <script type="text/javascript" src="toolkit/loader.js"></script>
  <script type="text/javascript">
      OAT.Preferences.imagePath = "toolkit/images/";
  </script>
  </head>
  <body>
    <v:page name="dav_browser"
      style="yacutia_style.xsl"
      fast-render="1"
      doctype="-//W3C//DTD XHTML 1.0 Transitional//EN"
      xmlns:xhtml="http://www.w3.org/1999/xhtml"
      xmlns:v="http://www.openlinksw.com/vspx/"
      xmlns:vm="http://www.openlinksw.com/vspx/macro">
      <v:template type="simple" condition="not self.vc_is_valid">
        <div class="warning">
          <v:error-summary/>
        </div>
      </v:template>

      <vm:popup_page_wrapper>
        <v:form name="F1" type="simple" method="POST" xhtml_enctype="multipart/form-data">
          <v:variable name="settings" type="any" default="null" />
          <v:variable name="domain_id" type="integer" default="0" />
          <v:variable name="owner_id" type="integer" default="null" />
          <v:variable name="account_id" type="integer" default="null" />
          <v:variable name="account_role" type="varchar" default="null" />
          <v:variable name="account_name" type="varchar" default="null"/>
          <v:variable name="account_password" type="varchar" default="null"/>

          <v:login
            name="admin_login_isql_browser"
            realm="virtuoso_admin"
            mode="url"
            user-password="y_sql_user_password"
            user-password-check="y_sql_user_password_check"
            xmlns:v="http://www.openlinksw.com/vspx/"
            xmlns:xhtml="http://www.w3.org/1999/xhtml">
            <v:template name='inl_browser' type="if-no-login">
              <P>You are not logged in</P>
            </v:template>
            <v:login-form name="loginf_browser"
                          required="1"
                          title="Login"
                          user-title="User Name"
                          password-title="Password"
                          submit-title="Login"/>
          </v:login>

          <v:before-data-bind>
            <![CDATA[
              declare vspx_user varchar;

              vspx_user := connection_get ('vspx_user');
              self.account_id := case when (isnull (vspx_user)) then http_nobody_uid () else WEBDAV.DBA.account_id (vspx_user) end;
              self.owner_id := self.account_id;
              self.settings := WEBDAV.DBA.settings (self.account_id);
              self.account_name := WEBDAV.DBA.account_name (self.account_id);
              self.account_password := WEBDAV.DBA.account_password (self.account_id);
            ]]>
          </v:before-data-bind>

          <?vsp if (connection_get ('vspx_user') is not null) { ?>
        <vm:dav_browser view="popup"/>
          <?vsp } ?>

        </v:form>
      </vm:popup_page_wrapper>
    </v:page>
  </body>
</html>
