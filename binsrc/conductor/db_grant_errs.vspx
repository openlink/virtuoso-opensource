<v:page name="db_grant_errs_page"
  decor="yacutia_decor.vspx"
  style="yacutia_style.xsl"
  fast-render="1"
  xmlns:v="http://www.openlinksw.com/vspx/"
  xmlns:vm="http://www.openlinksw.com/vspx/macro"
  doctype="-//W3C//DTD XHTML 1.0 Transitional//EN">
  <vm:pagetitle>Remove Database Object</vm:pagetitle>
  <vm:pagewrapper>
    <vm:variables>
      <v:variable persist="1" name="db_object_errs" type="any" default="null"/>
    </vm:variables>
    <vm:menu>
      <vm:menuitem value="Privileges On Database Objects"/>
    </vm:menu>
    <vm:header caption="Privileges On Database Objects"/>
    <vm:pagebody>
      	<v:form name="drop_obj" method="POST" type="simple">
          <div class="confirm_dialog">
            <img class="dialog_icon" src="images/icons/about_32.png" alt="Stop" title="Stop"/>
            <div class="confirm_text">
	      There was a problem with the following queries:.
	    </div>
            <div class="dialog_buttons">
              <v:button action="simple" name="cancel" value="Go back">
                <v:on-post>
		  self.vc_redirect ('databases.vspx');
                </v:on-post>
              </v:button>
	    </div>
	  </div>
	    <table class="listing">
	      <?vsp
	        declare i, l int;
		declare t any;

		l := length (self.db_object_errs);

	        for (i := 0; i < l; i := i + 1)
		   {
	       ?>
	       <tr>
		       <td colspan="2"><?V self.db_object_errs[i][0] ?></td>
		     </tr>
		     <tr>
		       <td align="right"><?V self.db_object_errs[i][1] ?></td>
		       <td><?V self.db_object_errs[i][2] ?></td>
	       </tr>
	      <?vsp
	           }
	        self.db_object_errs := null;
 	      ?>
	    </table>
	</v:form>
    </vm:pagebody>
  </vm:pagewrapper>
</v:page>
